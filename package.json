{
  "name": "digital-human-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite --mode development --host",
    "test": "cross-env NODE_ENV=test vite --mode test --host",
    "uat": "cross-env NODE_ENV=uat vite --mode uat --host",
    "prod": "cross-env NODE_ENV=production vite --mode production --host",
    "build:dev": "cross-env NODE_ENV=development vue-tsc --noEmit && vite build --mode development",
    "build:test": "cross-env NODE_ENV=test vue-tsc --noEmit && vite build --mode test",
    "build:uat": "cross-env NODE_ENV=uat vue-tsc --noEmit && vite build --mode uat",
    "build:prod": "cross-env NODE_ENV=production vue-tsc --noEmit && vite build --mode production",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "svg": "node removeSvgColorProperties.js"
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.3",
    "@codemirror/lang-javascript": "^6.1.9",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-xml": "^6.0.2",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "1.4.0",
    "codemirror": "^6.0.1",
    "crypto-js": "^4.1.1",
    "element-plus": "2.3.5",
    "pinia": "2.0.36",
    "unplugin-icons": "^0.16.1",
    "uuid": "^9.0.0",
    "vue": "3.3.2",
    "vue-codemirror": "^6.1.1",
    "vue-cropper": "^1.0.9",
    "vue-i18n": "9.2.2",
    "vue-router": "4.2.0",
    "webrtcsdk_new": "^1.6.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "2.0.1",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "18.16.8",
    "@types/postcss-less": "4.0.2",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/tsconfig": "0.4.0",
    "autoprefixer": "10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "less": "4.1.3",
    "npm-run-all": "^4.1.5",
    "postcss-pxtorem": "6.0.0",
    "prettier": "^2.8.8",
    "terser": "^5.17.6",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4",
    "unplugin-auto-import": "0.16.2",
    "unplugin-vue-components": "0.24.1",
    "vite": "4.3.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "1.6.4"
  }
}
